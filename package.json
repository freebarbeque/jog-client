{
  "name": "jog",
  "version": "0.0.1",
  "scripts": {
    "ci": "yarn flow && yarn eslint",
    "precommit": "lint-staged && yarn lint",
    "commit": "yarn flow && yarn fix && git commit -a",
    "deploy:android:alpha": "./scripts/deploy-android-alpha.sh",
    "deploy:ios:beta": "./scripts/deploy-ios-beta.sh",
    "deploy:web:dev": "yarn build:web:dev && firebase deploy -p build -P jog-dev-10fe9 --only hosting",
    "deploy:web:prod": "yarn build:web:prod && firebase deploy -p build -P jog-prod --only hosting",
    "fix": "yarn fix:eslint && yarn fix:sort-package-json",
    "fix:eslint": "eslint ./ --fix",
    "fix:sort-package-json": "sort-package-json",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "format:js": "prettier --single-quote --trailing-comma all --no-semi --write \"src/**/*.js\"",
    "generate:policies": "NODE_PATH=../ babel-node scripts/mock/generateMockPolicies.js -b es2015",
    "postinstall": "remotedev-debugger --hostname localhost --port 5678 --injectserver",
    "lint": "yarn lint:js",
    "lint:js": "eslint ./src",
    "start": "yarn start:packager",
    "start:packager": "node node_modules/react-native/local-cli/cli.js start"
  },
