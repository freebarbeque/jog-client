{
  "name": "ReactNativeTS",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "8.6.0"
  },
  "scripts": {
    "start:native": "node node_modules/react-native/local-cli/cli.js start --skipflow & yarn run build:native -- --watch",
    "start:web": "NODE_ENV=local webpack-dev-server",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "build:native": "rm -rf build && tsc -p tsconfig.rn.json",
    "build:web": "webpack",
    "pretest": "npm run build:native && npm run build:web",
    "test": "jest",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "pre-push-lint-msg": "echo 'Pre-push is running lint...' && exit 0",
    "pre-push-test-msg": "echo 'Pre-push is running test...' && exit 0"
  },
  "pre-push": [
    "pre-push-lint-msg",
    "lint",
    "pre-push-test-msg",
    "test"
  ],
  "dependencies": {
    "@types/ramda": "0.24.14",
    "@types/react-fontawesome": "1.6.0",
    "@types/react-navigation": "1.0.20",
    "@types/react-redux": "5.0.9",
    "@types/redux-promise": "0.5.27",
    "amazon-cognito-identity-js": "1.21.0",
    "aws-sdk": "2.133.0",
    "babel-polyfill": "6.23.0",
    "classnames": "2.2.5",
    "date-fns": "^1.29.0",
    "dotenv": "^4.0.0",
    "global": "4.3.2",
    "install": "0.10.1",
    "jest-cli": "21.2.1",
    "material-ui": "0.19.4",
    "moment": "^2.19.2",
    "npm": "5.4.2",
    "ramda": "0.24.1",
    "react": "16.2.0",
    "react-datepicker": "^0.61.0",
    "react-dom": "^16.2.0",
    "react-dropzone": "^4.2.3",
    "react-fontawesome": "1.6.1",
    "react-modal": "^3.1.8",
    "react-native": "0.44.0",
    "react-native-cli": "2.0.1",
    "react-native-collapsible": "0.9.0",
    "react-native-linear-gradient": "2.3.0",
    "react-native-media-queries": "0.3.0",
    "react-native-modal-dropdown": "0.5.0",
    "react-native-pie": "0.4.0",
    "react-native-pie-chart": "1.0.12",
    "react-navigation": "1.0.0-beta.11",
    "react-pdf": "^2.4.1",
    "react-redux": "5.0.6",
    "react-router": "4.1.1",
    "react-router-dom": "4.1.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scroll": "^1.7.5",
    "react-select": "^1.2.1",
    "react-spinners": "^0.2.5",
    "react-tap-event-plugin": "^3.0.2",
    "recompose": "0.26.0",
    "redux": "3.7.2",
    "redux-form": "7.1.2",
    "redux-freeze": "0.1.5",
    "redux-logger": "3.0.6",
    "redux-persist": "^5.4.0",
    "redux-promise": "0.5.3",
    "redux-saga": "0.15.3",
    "redux-thunk": "2.2.0",
    "reselect": "^3.0.1",
    "styled-components": "2.1.1",
    "tslint": "5.8.0",
    "tslint-react": "3.2.0",
    "uuid": "^3.1.0",
    "validate.js": "^0.12.0",
    "webpack-dev-server": "2.6.1",
    "yo": "2.0.0"
  },
  "devDependencies": {
    "@types/jest": "21.1.5",
    "@types/node": "^8.0.51",
    "@types/react": "15.0.24",
    "@types/react-native": "0.48.8",
    "@types/react-test-renderer": "15.5.0",
    "awesome-typescript-loader": "3.2.2",
    "babel-jest": "19.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react-native": "1.9.1",
    "babel-preset-stage-1": "^6.24.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "css-loader": "0.28.4",
    "generator-rn-toolbox": "3.1.0",
    "html-webpack-plugin": "2.30.1",
    "jest": "21.2.1",
    "pre-push": "^0.1.1",
    "react-hot-loader": "^3.1.3",
    "react-native-testfairy": "2.2.0",
    "react-test-renderer": "16.0.0-alpha.6",
    "remote-redux-devtools": "0.5.12",
    "source-map-loader": "0.2.1",
    "style-loader": "0.18.2",
    "typescript": "2.3.2",
    "url-loader": "0.5.9",
    "webpack": "3.4.1"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "testRegex": "build/.+(__tests__.*|\\.(test|spec))\\.js$",
    "moduleFileExtensions": [
      "js"
    ],
    "transformIgnorePatterns": [
      "node_modules/?!(react-native|native-base)"
    ]
  }
}
