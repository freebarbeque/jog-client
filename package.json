{
  "name": "ReactNativeTS",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start:native": "node node_modules/react-native/local-cli/cli.js start --skipflow & yarn run build:native -- --watch",
    "start:web": "webpack-dev-server",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "build:native": "rm -rf build && tsc -p tsconfig.rn.json",
    "build:web": "webpack",
    "pretest": "npm run build:native && npm run build:web",
    "test": "jest",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "@types/material-ui": "0.17.19",
    "@types/ramda": "0.24.14",
    "@types/react-navigation": "1.0.20",
    "@types/react-redux": "5.0.9",
    "@types/redux-promise": "0.5.27",
    "amazon-cognito-identity-js": "1.21.0",
    "aws-sdk": "2.133.0",
    "babel-polyfill": "6.23.0",
    "global": "4.3.2",
    "install": "0.10.1",
    "jest-cli": "21.2.1",
    "npm": "5.4.2",
    "ramda": "0.24.1",
    "react": "16.0.0-alpha.6",
    "react-dom": "15.6.1",
    "react-native": "0.44.0",
    "react-native-cli": "2.0.1",
    "react-native-collapsible": "0.9.0",
    "react-native-linear-gradient": "2.3.0",
    "react-native-media-queries": "0.3.0",
    "react-native-modal-dropdown": "0.5.0",
    "react-native-pie": "0.4.0",
    "react-native-pie-chart": "1.0.12",
    "react-navigation": "1.0.0-beta.11",
    "react-redux": "5.0.6",
    "recompose": "0.26.0",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-promise": "0.5.3",
    "redux-thunk": "2.2.0",
    "tslint": "5.8.0",
    "tslint-react": "3.2.0",
    "webpack-dev-server": "2.6.1",
    "yo": "2.0.0",
    "material-ui": "0.18.7"
  },
  "devDependencies": {
    "@types/jest": "21.1.5",
    "@types/react": "15.0.24",
    "@types/react-native": "0.48.8",
    "@types/react-test-renderer": "15.5.0",
    "awesome-typescript-loader": "3.2.2",
    "babel-jest": "19.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react-native": "1.9.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "css-loader": "0.28.4",
    "generator-rn-toolbox": "3.1.0",
    "html-webpack-plugin": "2.30.1",
    "jest": "21.2.1",
    "react-native-testfairy": "2.2.0",
    "react-test-renderer": "16.0.0-alpha.6",
    "remote-redux-devtools": "0.5.12",
    "source-map-loader": "0.2.1",
    "style-loader": "0.18.2",
    "typescript": "2.3.2",
    "url-loader": "0.5.9",
    "webpack": "3.4.1"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": ["<rootDir>/jest/setup.js"],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "testRegex": "build/.+(__tests__.*|\\.(test|spec))\\.js$",
    "moduleFileExtensions": [
      "js"
    ],
    "transformIgnorePatterns": [
      "node_modules/?!(react-native|native-base)"
    ]
  }
}
