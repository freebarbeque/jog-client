{
  "name": "jog",
  "version": "0.0.1",
  "scripts": {
    "ci": "yarn flow && yarn eslint && yarn test",
    "commit": "yarn flow && yarn fix && git commit -a",
    "eslint": "eslint ./",
    "fix": "yarn fix:eslint && yarn fix:sort-package-json",
    "fix:eslint": "eslint ./ --fix",
    "fix:sort-package-json": "sort-package-json",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "run:android": "react-native-scripts android",
    "run:ios": "react-native-scripts ios",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watch",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "deploy:android:alpha": "fastlane supply --apk android/app/app-release.apk --track alpha --json_key $JOG_GP_JSON_KEY --package_name com.jog",
    "build:android": "./scripts/android-release.sh"
  },
  "dependencies": {
    "@expo/ex-navigation": "^2.9.4",
    "email-validator": "^1.0.7",
    "firebase": "^3.7.3",
    "lodash": "^4.17.4",
    "react": "~15.4.0",
    "react-native": "^0.42.3",
    "react-native-fade-in-view": "^1.0.4",
    "react-native-keyboard-aware-scroll-view": "^0.2.8",
    "react-native-svg-uri": "^1.2.0",
    "react-native-vector-icons": "^4.0.0",
    "react-navigation": "^1.0.0-beta.7",
    "react-redux": "^5.0.3",
    "react-style-proptype": "^2.0.2",
    "redux": "^3.6.0",
    "redux-freeze": "^0.1.5",
    "redux-logger": "^3.0.0",
    "redux-persist": "^4.5.0",
    "redux-saga": "^0.14.4",
    "remote-redux-devtools": "^0.5.7"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-node": "^0.3.0",
    "eslint-import-resolver-reactnative": "^1.0.1",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "flow-bin": "^0.38.0",
    "jest-cli": "^19.0.2",
    "jest-expo": "^0.3.0",
    "react-test-renderer": "~15.4.1",
    "sort-package-json": "^1.5.0"
  },
  "engines": {
    "node": ">=7.7.0"
  },
  "rnpm": {
    "assets": [
      "assets/fonts"
    ]
  },
  "engineStrict": true,
  "private": true,
  "jest": {
    "preset": "jest-expo",
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__tests__/config/stubs/CSSStub.js",
      "^.+\\.css$": "<rootDir>/__tests__/config/stubs/CSSStub.js"
    },
    "setupFiles": [
      "<rootDir>/__tests__/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "node",
    "testRegex": ".*.test.js",
    "verbose": true,
    "transformIgnorePatterns": [
      "node_modules/(?!react-navigation)/"
    ]
  }
}
