{
  "name": "jog",
  "version": "0.0.1",
  "scripts": {
    "build:web:prod": "JOG_ENVIRONMENT=RELEASE neutrino build",
    "build:web:dev": "JOG_ENVIRONMENT=DEBUG neutrino build",
    "ci": "yarn flow && yarn eslint",
    "precommit": "lint-staged && yarn lint",
    "commit": "yarn flow && yarn fix && git commit -a",
    "deploy:android:alpha": "./scripts/deploy-android-alpha.sh",
    "deploy:ios:beta": "./scripts/deploy-ios-beta.sh",
    "deploy:web:dev": "yarn build:web:dev && firebase deploy -p build -P jog-dev-10fe9 --only hosting",
    "deploy:web:prod": "yarn build:web:prod && firebase deploy -p build -P jog-prod --only hosting",
    "fix": "yarn fix:eslint && yarn fix:sort-package-json",
    "fix:eslint": "eslint ./ --fix",
    "fix:sort-package-json": "sort-package-json",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "format:js": "prettier --single-quote --trailing-comma all --no-semi --write \"src/**/*.js\"",
    "generate:policies": "NODE_PATH=../ babel-node scripts/mock/generateMockPolicies.js -b es2015",
    "postinstall": "remotedev-debugger --hostname localhost --port 5678 --injectserver",
    "lint": "yarn lint:js",
    "lint:js": "eslint ./",
    "start": "yarn start:packager",
    "start:packager": "node node_modules/react-native/local-cli/cli.js start",
    "start:web": "neutrino start",
    "test": "echo \"No tests yet \"",
    "test:watch": "echo \"No tests yet \""
  },
  "dependencies": {
    "async-throttle": "^1.1.0",
    "better-react-spinkit": "^2.0.0",
    "classnames": "^2.2.5",
    "email-validator": "^1.0.7",
    "eslint-plugin-prettier": "^2.1.1",
    "firebase": "^3.7.3",
    "firebase-admin": "^5.0.0",
    "firebase-functions": "^0.5.7",
    "jest-react-native": "^18.0.0",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "material-ui": "0.18.3",
    "moment": "^2.18.1",
    "prop-types": "^15.5.8",
    "react": "16.0.0-alpha.6",
    "react-addons-css-transition-group": "^15.5.2",
    "react-collapsible": "^1.4.0",
    "react-dom": "^15.5.4",
    "react-fontawesome": "^1.6.1",
    "react-hot-loader": "next",
    "react-native": "0.44.0",
    "react-native-camera": "^0.6.0",
    "react-native-collapsible": "^0.8.0",
    "react-native-config": "^0.3.1",
    "react-native-datepicker": "^1.4.7",
    "react-native-device-info": "^0.10.2",
    "react-native-document-picker": "^1.0.2",
    "react-native-fade-in-view": "^1.0.4",
    "react-native-fcm": "^6.2.3",
    "react-native-fetch-blob": "^0.10.4",
    "react-native-file-picker": "^0.0.5",
    "react-native-firestack": "^2.3.9",
    "react-native-hyperlink": "^0.0.9",
    "react-native-image-picker": "^0.26.3",
    "react-native-keyboard-aware-scroll-view": "^0.2.8",
    "react-native-mime-types": "^2.2.0",
    "react-native-pdf-view": "^0.3.2",
    "react-native-photo-view": "^1.2.0",
    "react-native-picker": "^4.0.18",
    "react-native-spinkit": "^1.0.2",
    "react-native-svg-uri": "^1.2.0",
    "react-native-vector-icons": "^4.0.0",
    "react-native-webview-autoheight": "^1.0.4",
    "react-navigation": "^1.0.0-beta.9",
    "react-redux": "5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "5.0.0-alpha.4",
    "react-style-proptype": "^2.0.2",
    "react-swipeable-views": "^0.12.3",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-freeze": "^0.1.5",
    "redux-logger": "^3.0.0",
    "redux-persist": "^4.5.0",
    "redux-saga": "0.15.3",
    "remote-redux-devtools": "0.5.11",
    "remote-redux-devtools-on-debugger": "^0.7.0",
    "reselect": "^3.0.0",
    "styled-components": "^2.0.1",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "20.0.3",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-native": "1.9.2",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-prettier": "^1.7.0",
    "eslint-import-resolver-node": "^0.3.0",
    "eslint-import-resolver-reactnative": "1.0.2",
    "eslint-plugin-flowtype": "2.34.0",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "flow-bin": "^0.42.0",
    "husky": "^0.13.4",
    "jest": "20.0.4",
    "lint-staged": "3.6.0",
    "neutrino": "^6.0.2",
    "neutrino-middleware-pwa": "^6.0.3",
    "neutrino-preset-react": "^6.0.2",
    "prettier": "1.4.2",
    "react-test-renderer": "16.0.0-alpha.6",
    "sort-package-json": "^1.5.0"
  },
  "engines": {
    "node": ">=7.7.0"
  },
  "engineStrict": true,
  "private": true,
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__tests__/config/stubs/CSSStub.js",
      "^.+\\.css$": "<rootDir>/__tests__/config/stubs/CSSStub.js"
    },
    "setupFiles": [
      "<rootDir>/__tests__/config/polyfills.js",
      "<rootDir>/__tests__/config/setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "node",
    "testRegex": ".*.test.js",
    "verbose": true,
    "transformIgnorePatterns": [
      "node_modules/(?!react-navigation)/"
    ]
  },
  "lint-staged": {
    "src/**/*.js": [
      "yarn format:js --",
      "git add"
    ]
  },
  "rnpm": {
    "assets": [
      "assets/fonts"
    ]
  }
}
