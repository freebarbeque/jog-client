{
	"name": "jog",
	"version": "0.0.1",
	"scripts": {
		"ci": "yarn flow && yarn eslint && yarn test",
		"commit": "yarn flow && yarn fix && git commit -a",
		"deploy:android:alpha": "./scripts/deploy-android-alpha.sh",
		"deploy:ios:beta": "./scripts/deploy-ios-beta.sh",
		"eslint": "eslint ./",
		"fix": "yarn fix:eslint && yarn fix:sort-package-json",
		"fix:eslint": "eslint ./ --fix",
		"fix:sort-package-json": "sort-package-json",
		"flow": "flow",
		"flow-typed": "flow-typed",
		"generate:policies": "NODE_PATH=../ babel-node scripts/mock/generateMockPolicies.js -b es2015",
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"test": "jest",
		"test:watch": "jest --watch",
		"postinstall": "remotedev-debugger --hostname localhost --port 5678 --injectserver"
	},
	"dependencies": {
		"async-throttle": "^1.1.0",
		"email-validator": "^1.0.7",
		"firebase": "^3.7.3",
		"jest-react-native": "^18.0.0",
		"lodash": "^4.17.4",
		"moment": "^2.18.1",
		"prop-types": "^15.5.8",
		"react": "16.0.0-alpha.6",
		"react-native": "0.44.0",
		"react-native-camera": "^0.6.0",
		"react-native-config": "^0.3.1",
		"react-native-device-info": "^0.10.2",
		"react-native-document-picker": "^1.0.2",
		"react-native-fade-in-view": "^1.0.4",
		"react-native-fetch-blob": "^0.10.4",
		"react-native-file-picker": "^0.0.5",
		"react-native-firestack": "^2.3.9",
		"react-native-image-picker": "^0.26.3",
		"react-native-keyboard-aware-scroll-view": "^0.2.8",
		"react-native-mime-types": "^2.2.0",
		"react-native-pdf-view": "^0.3.2",
		"react-native-photo-view": "^1.2.0",
		"react-native-picker": "^4.0.18",
		"react-native-spinkit": "^1.0.2",
		"react-native-svg-uri": "^1.2.0",
		"react-native-vector-icons": "^4.0.0",
		"react-navigation": "^1.0.0-beta.9",
		"react-redux": "^5.0.3",
		"react-style-proptype": "^2.0.2",
		"redux": "^3.6.0",
		"redux-freeze": "^0.1.5",
		"redux-logger": "^3.0.0",
		"redux-persist": "^4.5.0",
		"redux-saga": "^0.14.4",
		"remote-redux-devtools": "^0.5.7",
		"remote-redux-devtools-on-debugger": "^0.7.0",
		"reselect": "^3.0.0",
		"uuid": "^3.0.1"
	},
	"devDependencies": {
		"babel-eslint": "^7.2.0",
		"babel-jest": "19.0.0",
		"babel-preset-es2015": "^6.24.0",
		"babel-preset-react-native": "1.9.1",
		"eslint": "^3.18.0",
		"eslint-config-airbnb": "^14.1.0",
		"eslint-import-resolver-node": "^0.3.0",
		"eslint-import-resolver-reactnative": "^1.0.1",
		"eslint-plugin-flowtype": "^2.30.4",
		"eslint-plugin-import": "^2.2.0",
		"eslint-plugin-jsx-a11y": "^4.0.0",
		"eslint-plugin-react": "^6.10.3",
		"flow-bin": "^0.42.0",
		"jest": "19.0.2",
		"react-test-renderer": "16.0.0-alpha.6",
		"sort-package-json": "^1.5.0"
	},
	"engines": {
		"node": ">=7.7.0"
	},
	"engineStrict": true,
	"private": true,
	"jest": {
		"preset": "react-native",
		"moduleFileExtensions": [
			"jsx",
			"js",
			"json"
		],
		"moduleNameMapper": {
			"^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__tests__/config/stubs/CSSStub.js",
			"^.+\\.css$": "<rootDir>/__tests__/config/stubs/CSSStub.js"
		},
		"setupFiles": [
			"<rootDir>/__tests__/config/polyfills.js",
			"<rootDir>/__tests__/config/setup.js"
		],
		"testPathIgnorePatterns": [
			"<rootDir>/(build|docs|node_modules)/"
		],
		"testEnvironment": "node",
		"testRegex": ".*.test.js",
		"verbose": true,
		"transformIgnorePatterns": [
			"node_modules/(?!react-navigation)/"
		]
	},
	"rnpm": {
		"assets": [
			"assets/fonts"
		]
	}
}
